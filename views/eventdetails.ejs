<html>
  <body>    
    <table border="1">
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Food Preference</th>
        </tr>
      </thead>
      <tbody>
      <% for(let participant of event.participants) { %>
        <tr>
          <td><%= participant.name %></td>
          <td><%= participant.email %></td>
          <td><%= participant.foodPreference %></td>          
        </tr>
      <% } %>
        </tbody>
    </table>
  </body>
</html>